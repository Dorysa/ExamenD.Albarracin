	Algoritmo PositivoNegativo
		
		Definir num Como Entero
		
		Escribir "Digita un numero: "
		leer num
		
		Si num>0 Entonces
			Escribir "El numero: ",num, " Es positivo"
		SiNo
			Si num<0 Entonces
				Escribir "El numero: ",num, " Es Negativo"
			SiNo
				Escribir "El numero: ",num, " Es Neutro"
			Fin Si
		Fin Si
		

FinAlgoritmo
