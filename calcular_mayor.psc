	Algoritmo calcular_mayor
				Definir num1, num2, mayor Como Entero
		
				Escribir "Ingrese el primer número:"
		Leer num1
		Escribir "Ingrese el segundo número:"
		Leer num2
		
				Si num1 > num2 Entonces
			mayor <- num1
		Sino
			mayor <- num2
		FinSi
		
				Escribir "El mayor número entre ", num1, " y ", num2, " es ", mayor
		
FinAlgoritmo

